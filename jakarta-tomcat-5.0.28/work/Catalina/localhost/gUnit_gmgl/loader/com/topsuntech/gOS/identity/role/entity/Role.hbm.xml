<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.topsuntech.gOS.identity.role.entity.Role" table="GOS_N_ROLE">
		<id name="id" type="long">
			<generator class="sequence">
				<param name="sequence">GOS_ROLE_SEQ</param>
			</generator>
		</id>
		<property name="name" column="NAME" type="string" />
		<property name="createDate" column="CREATE_DATE" type="date" />
		<property name="creatorId" column="CREATOR_ID"  type="long" />
		<property name="description" column="DESCRIPTION" type="string" />
	</class>
</hibernate-mapping>