<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<!-- 
    Created by Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="com.topsuntech.gOS.user.en.region.Region" 
    table="GOS_REGION"
>
    <id
        name="id"
        type="long"
    >
		<generator class="sequence">
				<param name="sequence">GOS_REGION_SEQ</param>
		</generator>
			
    </id>
    <property
        name="name"
        type="string"
    />
    
    <property
        name="level"
        type="long"
        column="levelid"
    />
    
    <property
        name="description"
        type="string"
    />
    
    <property name="createdate"/>
    
    <property name="creatorid"/>
    
    <property name="parentid"/>

    <property name="num"/>
    
    <property name="shortname"/>
    
    <property 
    	name="order"
    	column="disporder"
    	type="string"
    />
    
</class>
</hibernate-mapping>
