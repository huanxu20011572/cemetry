<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.topsuntech.gUnit.gEU_taskFilter.log.entity.LogRecordContent" table="GOS_LOG_RECORD_CONTENT">
		<id name="id">
			<generator class="sequence">
				<param name="sequence">GOS_LOG_RECORD_SEQ</param>
			</generator>
		</id>
		<property name="logId" column="LOG_ID" />
		<property name="content" column="CONTENT" />
	</class>
</hibernate-mapping>