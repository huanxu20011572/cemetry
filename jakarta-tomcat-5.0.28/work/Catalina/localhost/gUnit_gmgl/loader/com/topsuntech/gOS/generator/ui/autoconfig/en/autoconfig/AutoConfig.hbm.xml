<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.topsuntech.gOS.generator.ui.autoconfig.en.autoconfig.AutoConfig" 
    table="GOS_AUTO_CONFIG"
>
    <id
        name="id"
        type="long"
    >
			<generator class="native" />
    </id>
    <property
        name="name"
        type="string"
    />
    <property
        name="engname"
        type="string"
    />
    <property
        name="gunitname"
        type="string"
    />
    <property
        name="technology"
        type="string"
    />
    <property
        name="category"
        type="string"
    />
    <property
        name="storage"
        type="string"
    />
    <property
        name="rootname"
        type="string"
    />
    <property
        name="filelink"
        type="string"
    />
    <property
        name="description"
        type="string"
    />
    <property
        name="createdate"
        type="date"
    />
    <property
        name="creatorid"
        type="long"
    />
    <property
        name="projectid"
        type="long"
    />
</class>
</hibernate-mapping>
